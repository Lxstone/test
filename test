import subprocess
import requests
import time
import sys
import os

hwid = str(str(subprocess.check_output('wmic csproduct get uuid')).strip().replace(r"\r", "").split(r"\n")[1].strip())
r = requests.get("https://pastebin.com/UmUAR2zu")

def printSlow(text):
    for char in text:
        print(char, end="")
        sys.stdout.flush()
        time.sleep(.1)

def Main_Program():
    if hwid in r.text:
        printSlow("Add Perfect Full...")
        time.sleep(.1)
        os.system('cls')
    else:
        print("Error! HWID Not I Database!")
        print("HWID: " + hwid)
        os.system('pause >NUL')

Main_Program()
cls
pause

